// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model User {
  id Int @id @default(autoincrement())
  username String? 
  nickname String?
  email String @unique
  phone String?
  password String @unique
  role String

  
}

model Aluno {
id String @id @default(uuid())
username String
nickname String
email String @unique
password String @unique
role String
}
model Coordenador {
id String @id @default(uuid())
username String
email String @unique
password String @unique
role String
professor Professor[]
}
model Professor {
 id String @id @default(uuid())
 username String
 email String @unique
 password String @unique
 role String
  coordenadorId String
  coordenador   Coordenador  @relation(fields: [coordenadorId], references: [id])
}

model Classroom{
  id String @id @default(uuid())
}
model Questao{
 id String @id @default(uuid())
}
model Simulado{
  id String @id @default(uuid())
}